  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/yaml": "5.0.*"
  },
  "autoload": {
    "psr-4": {
